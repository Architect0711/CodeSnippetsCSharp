<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>MSTest DataTestMethod</Title>
      <Shortcut>msdatatestmethod</Shortcut>
      <Description>Create a Method decorated with the DataTestMethod Attribute and an empty DataRow Attribute</Description>
      <Author>Robert Bantele</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>methodname</ID>
          <ToolTip>Meaningful Name for the Test</ToolTip>
          <Default>Methodname</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp">
        <![CDATA[
			[DataTestMethod]
      [DataRow()]
			public void $methodname$() 
			{
				// ---------- Arrange ----------
				
				// ------------ Act ------------
				
				// ---------- Assert -----------
			}
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>